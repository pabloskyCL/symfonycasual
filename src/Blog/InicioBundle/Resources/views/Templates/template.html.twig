<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bloginicio/css/style.css') }}">
    <title>{% block title %}{% endblock %}</title>
    <script src={{ asset('bundles/bloginicio/js/jquery-3.4.1.min.js') }}></script>
    <script src="{{ asset('bundles/bloginicio/js/jquery-ui.min.js') }}"></script>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col-md"></div>
                <div class="col-md-5"
                    {% block navbar %} {% endblock %}
                </div>
            <div class="col-md"></div>
        </div>
        <div class="row">
            <div class="col-md"></div>
                <div class="col-md-5">
                    {%  block notificacion %}
                    {% endblock %}
                    {% block contenido %}{% endblock %}
                </div>
            <div class="col-md"></div>
        </div>
        <div class="row">
            <div class="col-md"></div>
                <div class="col-md-5">
                    {% block footer %} {% endblock %}
                </div>
            <div class="col-md"></div>
        </div>
    </div>
    <script src={{ asset('bundles/bloginicio/js/alertas.js') }}></script>
</body>
</html>